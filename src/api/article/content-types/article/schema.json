{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "maxLength": 159,
      "minLength": 1
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "value": {
      "type": "integer",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "cover": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
